#include <bits/stdc++.h>
using namespace std;

void selectionsort(int arr[],int n){
    for(int i=0;i<n-1;i++){
        int minimum=i;
        for(int j=i+1;j<n;j++){
            if(arr[minimum]>arr[j]){
                swap(arr[minimum],arr[j]);
            }
       }
    }
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin>>n;

    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    selectionsort(arr,n);

    cout<<"Sortes array: ";

    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}

