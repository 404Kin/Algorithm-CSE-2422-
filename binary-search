#include<bits/stdc++.h>
using namespace std;
int binarySearch(int n,int arr[],int target){
    int low=0,high=n-1;
    while(low<=high){
        int mid=(low+high)/2;
        if(arr[mid]==target){
            return mid;
        }
        else if(arr[mid]>target){
            high=mid-1;
        }
        else{
            low=mid+1;
        }
    }
    return -1;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int target;
    cin>>target;
    sort(arr,arr+n);
    int result=binarySearch(n,arr,target);
    if(result!=-1){
        cout<<"found at "<<result<<endl;
    }
    else{
        cout<<"Not found"<<endl;
    }
    return 0;
}
